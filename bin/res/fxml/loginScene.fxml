<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" spacing="10.0" styleClass="body" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox fx:id="loginForm" alignment="TOP_CENTER" maxHeight="800.0" maxWidth="800.0" minHeight="-Infinity" minWidth="-Infinity" spacing="20.0" VBox.vgrow="ALWAYS">
         <children>
            <HBox fx:id="header" alignment="CENTER" maxHeight="-Infinity" minWidth="-Infinity" prefHeight="100.0" VBox.vgrow="NEVER">
               <VBox.margin>
                  <Insets top="-50.0" />
               </VBox.margin>
               <children>
                  <Label fx:id="txt" alignment="CENTER" contentDisplay="CENTER" maxHeight="-Infinity" minWidth="-Infinity" text="Welcome To " textAlignment="CENTER">
                     <font>
                        <Font name="Calibri" size="12.0" />
                     </font>
                  </Label>
                  <HBox fx:id="logo" alignment="CENTER">
                     <children>
                        <Label fx:id="frst" text="Ch" />
                        <Label minWidth="-Infinity" text="ats">
                           <font>
                              <Font name="Calibri Bold" size="12.0" />
                           </font>
                        </Label>
                     </children>
                  </HBox>
               </children>
            </HBox>
            <Label fx:id="state" />
            <TextField fx:id="username" maxWidth="300.0" promptText="Username" />
            <TextField fx:id="password" maxWidth="300.0" promptText="Password" />
            <VBox alignment="CENTER" maxHeight="200.0" maxWidth="-Infinity" spacing="10.0" VBox.vgrow="NEVER">
               <children>
                  <Button fx:id="signin" alignment="CENTER" contentDisplay="CENTER" focusTraversable="false" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minWidth="-Infinity" mnemonicParsing="false" onAction="#signIn" scaleShape="false" text="Sign In" VBox.vgrow="ALWAYS" />
                  <Button fx:id="signup" alignment="CENTER" contentDisplay="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minWidth="-Infinity" mnemonicParsing="false" text="Sign Up" VBox.vgrow="ALWAYS" />
                  <Button fx:id="frgpass" alignment="CENTER" contentDisplay="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minWidth="-Infinity" mnemonicParsing="false" text="Forgot Password" VBox.vgrow="ALWAYS" />
               </children>
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </VBox>
         </children>
         <VBox.margin>
            <Insets left="50.0" right="50.0" top="50.0" />
         </VBox.margin>
      </VBox>
   </children>
</VBox>
