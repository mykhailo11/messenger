<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.geometry.Rectangle2D?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollBar?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" maxHeight="800.0" styleClass="body" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox fx:id="userForm" alignment="TOP_CENTER" maxHeight="800.0" maxWidth="800.0" spacing="10.0" VBox.vgrow="ALWAYS">
         <children>
            <HBox fx:id="chats" alignment="CENTER" maxHeight="-Infinity" prefHeight="50.0" spacing="10.0" VBox.vgrow="NEVER">
               <children>
                  <ImageView fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true" styleClass="user">
                     <image>
                        <Image url="@../../../../../../Projects/Learning/media/images/email.png" />
                     </image>
                  </ImageView>
                  <ImageView fitHeight="75.0" fitWidth="75.0" pickOnBounds="true" preserveRatio="true" styleClass="user">
                     <image>
                        <Image url="@../../../../../../Projects/Learning/media/images/email.png" />
                     </image>
                  </ImageView>
                  <ImageView fitHeight="75.0" fitWidth="75.0" pickOnBounds="true" preserveRatio="true" styleClass="user">
                     <image>
                        <Image url="@../../../../../../Projects/Learning/media/images/email.png" />
                     </image>
                  </ImageView>
                  <ImageView fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true" styleClass="user">
                     <image>
                        <Image url="@../../../../../../Projects/Learning/media/images/email.png" />
                     </image>
                     <viewport>
                        <Rectangle2D />
                     </viewport>
                  </ImageView>
               </children>
               <VBox.margin>
                  <Insets top="-50.0" />
               </VBox.margin>
            </HBox>
            <HBox alignment="TOP_CENTER" depthTest="DISABLE" prefHeight="172.0" prefWidth="320.0" VBox.vgrow="ALWAYS">
               <children>
                  <VBox fx:id="notes" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin></VBox>
                  <ScrollBar minWidth="-Infinity" orientation="VERTICAL" HBox.hgrow="NEVER">
                     <cursor>
                        <Cursor fx:constant="DEFAULT" />
                     </cursor></ScrollBar>
               </children>
               <VBox.margin>
                  <Insets left="10.0" right="10.0" />
               </VBox.margin>
            </HBox>
            <HBox fx:id="mess" alignment="CENTER" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" VBox.vgrow="NEVER">
               <children>
                  <TextField maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" prefHeight="28.0" promptText="Enter your message..." HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin>
                  </TextField>
                  <Button mnemonicParsing="false" text="SEND">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Button>
               </children>
               <VBox.margin>
                  <Insets left="10.0" right="10.0" />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </children>
   <padding>
      <Insets left="50.0" right="50.0" top="50.0" />
   </padding>
</VBox>
